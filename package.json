{
  "name": "rocket-broadheads",
  "title": "Rocket Broadheads",
  "homepage": "https://rocket-broadheads.netlify.com",
  "description": "We make them lethal. You drop them quick.",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "git@github.com:escaladesports/rocket-broadheads.git",
  "author": "Kennedy Rose <kennedy@escaladesports.com>",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel watch server",
    "server": "node server.js",
    "salsify": "node utils/salsify && node utils/salsify-images",
    "copy": "node utils/copy",
    "prebuild": "npm run clean && npm-run-all --parallel markdown privacy-policy terms-of-service salsify && npm-run-all --parallel img product-to-json",
    "build": "next build && next export -o dist",
    "postbuild": "npm-run-all --parallel sitemap copy minify",
    "img": "node -e \"require('./utils/images')()\"",
    "reviews": "node utils/reviews",
    "watch": "node utils/watch",
    "sitemap": "node utils/sitemap",
    "markdown": "node utils/markdown",
    "posts": "node utils/posts",
    "minify": "html-minifier --input-dir ./dist --output-dir ./dist --file-ext html --config-file ./config/.htmlmin",
    "privacy-policy": "node utils/markdown-modules -m privacy-policy",
    "terms-of-service": "node utils/markdown-modules -m terms-of-service",
    "clean": "node utils/clean",
    "product-to-json": "node node_modules/product-to-json"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-plugin-dotenv": "^0.0.2",
    "babel-plugin-inline-react-svg": "^0.4.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-inline-environment-variables": "^0.1.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "chokidar": "^1.7.0",
    "cpy": "^5.0.0",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "form-serialize": "^0.7.2",
    "fs-extra": "^4.0.0",
    "glob-all": "^3.1.0",
    "glob-promise": "^3.1.0",
    "google-map-react": "^0.24.0",
    "gray-matter": "^3.0.2",
    "html-loader": "^0.4.5",
    "html-minifier": "^3.5.2",
    "is-email": "^1.0.0",
    "is-phone": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-loader": "^1.6.4",
    "lost": "^8.1.0",
    "markdown-to-json-files": "^1.1.1",
    "marked": "^0.3.6",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.0",
    "next": "beta",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "open-browser-webpack-plugin": "^0.0.5",
    "postcss-cssnext": "^2.11.0",
    "postcss-easy-import": "^2.1.0",
    "postcss-loader": "^2.0.6",
    "privacy-policy": "escaladesports/privacy-policy#v1.0.3",
    "product-to-json": "escaladesports/product-to-json#v0.1.0",
    "query-product": "escaladesports/query-product#v1.0.2",
    "raf": "^3.3.2",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-fastclick": "^3.0.2",
    "react-ga": "^2.2.0",
    "reviews-to-json": "escaladesports/reviews-to-json.git#1.1.2",
    "salsify-image-downloader": "escaladesports/salsify-image-downloader#v2.0.6",
    "salsify-to-json": "escaladesports/salsify-to-json#v2.0.4",
    "sass-loader": "^6.0.6",
    "sharp": "^0.18.2",
    "sitemap": "^1.13.0",
    "styled-jsx-postcss": "^0.2.0",
    "terms-of-service": "escaladesports/terms-of-service#v1.0.4",
    "usd-formatter": "^0.1.2"
  },
  "dependencies": {},
  "engines": {
    "node": "~8.1.3",
    "npm": "~5.0.3"
  }
}
